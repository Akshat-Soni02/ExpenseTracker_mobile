{
  "expo": {
    "name": "ExpenseEase",
    "slug": "ExpenseTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/l.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/l.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.searce.splitexpensetracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/l.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./firebase/google-services.json",
      "package": "com.searce.splitexpensetracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/inter/Inter_900Black.ttf","node_modules/@expo-google-fonts/inter/Inter_400Regular.ttf", "node_modules/@expo-google-fonts/inter/Inter_500Medium.ttf", "node_modules/@expo-google-fonts/inter/Inter_600SemiBold.ttf", "node_modules/@expo-google-fonts/inter/Inter_700Bold.ttf"]
        }
      ],
      "expo-router",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "18cb2f15-fe47-46ad-a879-4e2d4b67f4b2"
      }
    }
  }
}
